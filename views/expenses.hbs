<!-- expenses-->
<section class="page-section" id="expenses">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Expenses</h2>
            <h3 class="section-subheading text-muted">Check expenses on participated events.</h3>
        </div>
        <ul class="timeline" id="timelineMainDiv">
            {{#each expenses}}
            <li>
                <div class="timeline-image"><img class="rounded-circle img-fluid" src="assets/img/about/1.jpg" alt="" />
                </div>
                <div class="timeline-panel">
                    <div class="timeline-heading">
                        <h4>{{cost}}</h4>
                        <h4 class="subheading">{{name}}</h4>
                    </div>
                    <div class="timeline-body">
                        <p class="text-muted">Sponsored by: {{sponsoredBy}}</p>
                    </div>
                </div>
            </li>
            {{/each}}
        </ul>
    </div>
</section>